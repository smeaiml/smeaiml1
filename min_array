//Our given task is to find the minimum element in the given Array

public class min_array{


public static void main(String args[])    {

int arr[]={9,5,6,8,9}; 
                    
int min=arr[0];  
                                      
for(int i=0;i<arr.length;i++)  {

if(arr[i]<min)

min=arr[i];

}
System.out.println("Minimum element is ="+min);
}

}
